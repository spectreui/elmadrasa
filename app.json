{
  "expo": {
    "name": "Elmadrasa",
    "slug": "school-app",
    "scheme": "elmadrasa",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "jsEngine": "hermes",
      "package": "com.elmadrasa.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "elmadrasa.vercel.app",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.elmadrasa.app",
      "associatedDomains": [
        "applinks:elmadrasa-link.vercel.app",
        "applinks:elmadrasa.vercel.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "3"
    },
    "web": {
      "pwa": {
        "name": "Elmadrasa",
        "shortName": "Elmadrasa",
        "themeColor": "#007AFF",
        "backgroundColor": "#F2F2F7",
        "display": "standalone",
        "startUrl": ".",
        "orientation": "portrait",
        "scope": "/",
        "description": "Elmadrasa School App",
        "icons": [
          {
            "src": "./assets/icons/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/icons/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ad027a37-a112-4f47-a201-d904f5f43de6"
      }
    }
  }
}