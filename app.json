{
  "expo": {
    "name": "Elmadrasa",
    "slug": "school-app",
    "scheme": "elmadrasaapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "bundleIdentifier": "com.elmadrasa.app",
      "versionCode": 32,
      "jsEngine": "hermes"
    },
    "ios": {
      "bundleIdentifier": "com.elmadrasa.app",
      "associatedDomains": [
        "applinks:elmadrasa-link.vercel.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "2"
    },
    "web": {
      "pwa": {
        "name": "Elmadrasa",
        "shortName": "Elmadrasa",
        "themeColor": "#007AFF",
        "backgroundColor": "#F2F2F7",
        "display": "standalone",
        "startUrl": ".",
        "orientation": "portrait",
        "scope": "/",
        "description": "Elmadrasa School App",
        "icons": [
          {
            "src": "./assets/icons/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/icons/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01e5f25e-d1af-4d02-abb4-dac6931d1764"
      }
    }
  }
}